package digitales.logica.clases;

import master.logica.clases.Persona;

/**
 *
 * @author icits
 */
public class Articulo {

    private int codigo;
    private String titulo;
    private String introduccion;
    private int estado;
    private long fecha_registro;
    private long fecha_edicion;
    private Categoria codigo_categoria;
    private Persona codigo_persona;

    public Articulo() {
    }

    public Articulo(int codigo, String titulo, String introduccion, int estado, long fecha_registro, long fecha_edicion, Categoria codigo_categoria, Persona codigo_persona) {
        this.codigo = codigo;
        this.titulo = titulo;
        this.introduccion = introduccion;
        this.estado = estado;
        this.fecha_registro = fecha_registro;
        this.fecha_edicion = fecha_edicion;
        this.codigo_categoria = codigo_categoria;
        this.codigo_persona = codigo_persona;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    public String getTitulo() {
        return titulo;
    }

    public void setTitulo(String titulo) {
        this.titulo = titulo;
    }

    public String getIntroduccion() {
        return introduccion;
    }

    public void setIntroduccion(String introduccion) {
        this.introduccion = introduccion;
    }

    public int getEstado() {
        return estado;
    }

    public void setEstado(int estado) {
        this.estado = estado;
    }

    public long getFecha_registro() {
        return fecha_registro;
    }

    public void setFecha_registro(long fecha_registro) {
        this.fecha_registro = fecha_registro;
    }

    public long getFecha_edicion() {
        return fecha_edicion;
    }

    public void setFecha_edicion(long fecha_edicion) {
        this.fecha_edicion = fecha_edicion;
    }

    public Categoria getCodigo_categoria() {
        return codigo_categoria;
    }

    public void setCodigo_categoria(Categoria codigo_categoria) {
        this.codigo_categoria = codigo_categoria;
    }

    public Persona getCodigo_persona() {
        return codigo_persona;
    }

    public void setCodigo_persona(Persona codigo_persona) {
        this.codigo_persona = codigo_persona;
    }
    
}
