package digitales.logica.clases;

import master.logica.clases.Persona;

/**
 *
 * @author icits
 */

public class Texto {
private int codigo;
private String titulo;
private String resumen;
private String contenido;
private String keywords;
private long fecha_registro;
private long fecha_edicion;
private int estado;
private Persona codigo_persona;

    public Texto() {
        
    }

    public Texto(int codigo, String titulo, String resumen, String contenido, String keywords, long fecha_registro, long fecha_edicion, int estado, Persona codigo_persona) {
    this.codigo = codigo;
    this.titulo = titulo;
    this.resumen = resumen;
    this.contenido = contenido;
    this.keywords = keywords;
    this.fecha_registro = fecha_registro;
    this.fecha_edicion = fecha_edicion;
    this.estado = estado;
    this.codigo_persona = codigo_persona;
    }

    public int getEstado() {
        return estado;
    }

    public void setEstado(int estado) {
        this.estado = estado;
    }

    public int getCodigo() {
        return codigo;
    }

    public void setCodigo(int codigo) {
        this.codigo = codigo;
    }

    public String getTitulo() {
        return titulo;
    }

    public void setTitulo(String titulo) {
        this.titulo = titulo;
    }

    public String getResumen() {
        return resumen;
    }

    public void setResumen(String resumen) {
        this.resumen = resumen;
    }

    public String getContenido() {
        return contenido;
    }

    public void setContenido(String contenido) {
        this.contenido = contenido;
    }

    public String getKeywords() {
        return keywords;
    }

    public void setKeywords(String keywords) {
        this.keywords = keywords;
    }

    public long getFecha_registro() {
        return fecha_registro;
    }

    public void setFecha_registro(long fecha_registro) {
        this.fecha_registro = fecha_registro;
    }

    public long getFecha_edicion() {
        return fecha_edicion;
    }

    public void setFecha_edicion(long fecha_edicion) {
        this.fecha_edicion = fecha_edicion;
    }

    public Persona getCodigo_persona() {
        return codigo_persona;
    }

    public void setCodigo_persona(Persona codigo_persona) {
        this.codigo_persona = codigo_persona;
    }

}
